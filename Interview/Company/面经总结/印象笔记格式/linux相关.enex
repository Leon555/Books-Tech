if [ -f &quot;$newfile&quot; ]; then&nbsp;&nbsp;&nbsp;<br clear="none"/>
echo &quot;ERROR: $newfile exists already&quot;&nbsp;&nbsp;&nbsp;<br clear="none"/>
else&nbsp;&nbsp;&nbsp;<br clear="none"/>
echo &quot;renaming $file to $newfile ...&quot;&nbsp;&nbsp;&nbsp;<br clear="none"/>
mv &quot;$file&quot; &quot;$newfile&quot;&nbsp;&nbsp;&nbsp;<br clear="none"/>
fi&nbsp;&nbsp;&nbsp;<br clear="none"/>
fi&nbsp;&nbsp;&nbsp;<br clear="none"/>
done&nbsp;</span></p><p><span style="font-size: 13px;">#!/bin/sh<br clear="none"/>
